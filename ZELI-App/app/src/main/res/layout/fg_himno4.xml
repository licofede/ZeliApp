<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cosas_fondo">

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton13"
                    android:src="@drawable/cosas_ambulancia"
                    android:layout_gravity="center"
                    android:onClick="cosas_ambulancia"
                    android:background="@drawable/rippple"


                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Ambulancia-Ambulance"
                    android:id="@+id/textView"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="20dp"
                    android:editable="false"
                    android:elegantTextHeight="false"
                    android:textStyle="bold" />

            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp">

        </FrameLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton14"
                    android:nestedScrollingEnabled="false"
                    android:onClick="cosas_autobomba_firetruck"
                    android:src="@drawable/cosas_autobomba_firetruck"
                    android:layout_gravity="center_horizontal|bottom"
                    android:background="@drawable/rippple_rojo"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Autobomba-Fire truck "
                    android:id="@+id/textView3"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="20dp"
                    android:textStyle="bold" />

            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton15"
                    android:src="@drawable/cosas_camiondebasura"
                    android:layout_gravity="center"
                    android:onClick="cosas_camiondebasura"
                    android:background="@drawable/rippple_verde"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Camion de Basura-Garbage Truck"
                    android:id="@+id/textView4"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="16dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton16"
                    android:src="@drawable/cosas_chocolate"
                    android:layout_gravity="center"
                    android:onClick="cosas_chocolate"
                    android:background="@drawable/rippple"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Chocolate-Chocolate"
                    android:id="@+id/textView5"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="25dp"
                    android:textStyle="bold" />

            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton17"
                    android:nestedScrollingEnabled="false"
                    android:onClick="cosas_cochedepolicia"
                    android:src="@drawable/cosas_cochedepolicia"
                    android:layout_gravity="center"
                    android:background="@drawable/rippple_rojo"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Coche de Policia-Police Car"
                    android:id="@+id/textView7"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="18dp"
                    android:textStyle="bold" />
            </FrameLayout>

        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton18"
                    android:src="@drawable/cosas_helado"
                    android:onClick="cosas_helado"
                    android:layout_gravity="center"
                    android:background="@drawable/rippple_verde"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Helado-Ice Cream"
                    android:id="@+id/textView6"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="25dp"
                    android:textStyle="bold" />
            </FrameLayout>

        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton19"
                    android:onClick="cosas_helicoptero"
                    android:src="@drawable/cosas_helicoptero"
                    android:layout_gravity="center"
                    android:background="@drawable/rippple"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Helicóptero-Helicopter"
                    android:id="@+id/textView8"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="20dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton20"
                    android:src="@drawable/cosas_telefono"
                    android:layout_gravity="center"
                    android:onClick="cosas_telefono"
                    android:background="@drawable/rippple_rojo"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Teléfono-Phone"
                    android:id="@+id/textView9"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="30dp"
                    android:textStyle="bold" />

            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton21"
                    android:onClick="cosas_tortadecumpleanos"
                    android:src="@drawable/cosas_torta"
                    android:layout_gravity="center"
                    android:background="@drawable/rippple_verde"/>

                <TextView
                    android:layout_width="196dp"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Torta de Cumpleaños                                                     Birthday Cake"
                    android:id="@+id/textView10"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="19dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton22"
                    android:src="@drawable/cosas_trampolin"
                    android:layout_gravity="center"
                    android:onClick="cosas_trampolin"
                    android:background="@drawable/rippple"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Trampolín-Trampoline"
                    android:id="@+id/textView11"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="23dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageButton
                    android:layout_width="300dp"
                    android:layout_height="300dp"
                    android:id="@+id/imageButton23"
                    android:src="@drawable/cosas_tren"
                    android:onClick="cosas_tren"
                    android:layout_gravity="center"
                    android:background="@drawable/rippple_rojo"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="79dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Tren-Train"
                    android:id="@+id/textView12"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="#fcfef6"
                    android:textSize="30dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="23dp" />

    </LinearLayout>

</ScrollView>